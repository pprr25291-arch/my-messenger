{
  "productName": "My Messenger",
  "version": "1.0.0",
  "identifier": "com.my.messenger",
  "build": {
    "frontendDist": "../frontend",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm start"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "My Messenger",
        "width": 1200,
        "height": 800,
        "url": "https://my-messenger-9g2n.onrender.com",
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}